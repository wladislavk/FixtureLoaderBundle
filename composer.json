{
    "name": "vkr/fixture-loader-bundle",
    "type": "symfony-bundle",
    "description": "A bundle for Symfony2/3 that adds loading mechanisms for MySQL Doctrine fixtures",
    "keywords": ["doctrine", "fixtures", "mysql", "symfony2", "symfony3"],
    "homepage": "https://github.com/wladislavk/FixtureLoaderBundle",
    "license": "MIT",
    "authors": [
        {"name": "Vladislav Kryshtanovskiy", "email": "wladislav.k@gmail.com"}
    ],
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "~2.8|~3.0",
        "doctrine/orm": ">=2.2.3",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/doctrine-fixtures-bundle": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.4"
    },
    "autoload": {
        "psr-4": {
            "VKR\\FixtureLoaderBundle\\": ""
        }
    }
}
